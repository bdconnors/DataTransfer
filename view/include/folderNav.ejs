<nav style="border: 1px solid #343a40;" class="navbar navbar-expand-lg navbar-dark bg-dark">
    <a class="navbar-brand" href="/projects/project/<%=folder.projectId%>"><i class="fa fa-archive"></i></a>
    <a class="navbar-brand" href="/projects/project/<%=folder.projectId%>/folders/folder/<%= folder.id%>"><i class="fa fa-folder "></i> <%= folder.name %></a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#projectDropDown" aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
        <i class="fa fa-chevron-circle-down"></i>
    </button>
    <div class="navbar-collapse collapse w-100 order-1 order-md-0 dual-collapse2">
        <ul></ul>
    </div>
    <div class="navbar-collapse collapse w-100 order-3 dual-collapse2" id="projectDropDown">
        <ul class="navbar-nav ml-auto">
            <%if(user.admin || folder.metadata.userFolder === user.id){
                var renameJson = JSON.stringify({name:folder.name,type:'Folder'});
