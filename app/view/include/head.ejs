<%
var scripts = false;
var menu = false;

if(options.menu){
    menu = options.menu;
}

if(options.scripts){
    scripts = options.scripts;
}

%>

<!DOCTYPE html>
<html lang="en">
<head>

    <title><%= title%></title>
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <script src="/include/lib/jquery/dist/jquery.js" type="text/javascript"></script>
    <script rel="stylesheet" href="include/lib/bootstrap/dist/js/bootstrap.js"></script>
    <% if(menu === true){ %>
        <script src="/include/menu/scripts/menu.js"></script>
    <% } %>
    <% if(scripts === true){ %>
        <script src="<%= scripts %>"></script>
    <%}%>
    <link rel="stylesheet" href="/include/lib/bootstrap/dist/css/bootstrap.css">
    <link rel="stylesheet" href="/include/styles/styles.css">

</head>

